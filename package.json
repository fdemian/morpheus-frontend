{
  "name": "morpheus-frontend",
  "version": "0.7.0",
  "private": true,
  "proxy": "http://localhost:8888",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.32",
    "@fortawesome/free-brands-svg-icons": "5.15.1",
    "@fortawesome/free-regular-svg-icons": "5.15.1",
    "@fortawesome/free-solid-svg-icons": "5.15.1",
    "@fortawesome/react-fontawesome": "0.1.14",
    "ant-design-pro": "2.3.2",
    "antd": "4.9.4",
    "babel-jest": "^26.6.3",
    "elementary-editor": "0.9.9-rc1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-responsive": "8.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.1",
    "swr": "0.3.11"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.js",
      "!src/serviceWorker.js",
      "!src/Routes/*",
      "!src/sagas/*",
      "!src/store/Configure.js",
      "!src/store/Fetch.js",
      "!src/store/utils.js",
      "!<rootDir>/node_modules/",
      "!src/Account/BindingView.js",
      "!src/Account/NotificationView.js",
      "!src/App/initialData.js",
      "!src/*/Actions.js",
      "!src/*/*/Actions.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "src/node_modules/(?!@elementary-editor)"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@testing-library/jest-dom": "5.11.8",
    "@testing-library/react": "11.2.2",
    "antd-dayjs-webpack-plugin": "1.0.2",
    "babel-plugin-import": "1.13.3",
    "customize-cra": "^1.0.0",
    "es-abstract": "1.18.0-next.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-fetch-mock": "^3.0.3",
    "react-app-rewired": "2.1.8"
  },
  "scripts": {
    "start-rewired": "react-app-rewired start",
    "build-rewired": "react-app-rewired build",
    "test-rewired": "react-app-rewired test",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:coverage": "yarn test --coverage --watchAll=false"
  },
  "browserslist": [
    "last 2 versions"
  ]
}
